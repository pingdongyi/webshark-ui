    "@types/jasmine": "~4.3.0",
    "@types/wavesurfer.js": "^6.0.6",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
